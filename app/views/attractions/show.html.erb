<%= @attraction.name %>

<%= form_tag user_path(User.find(session[:user_id])), method: :get do %>

  <% if !@admin %>
    <%= button_tag "Go on this ride", name: "attraction_id", value: @attraction.id %>
  <% else %>
    <%= link_to "Edit Attraction", edit_attraction_path(@attraction) %>
  <% end %>
<% end %>
